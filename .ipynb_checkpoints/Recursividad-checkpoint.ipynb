    "def _findSum(arr, N):\n",
    "     if len(arr)== 1:\n",
    "        return arr[0]\n",
    "     else:\n",
    "        return arr[0]+_findSum(arr[1:], N)\n",
    " \n",
    "arr = [1, 2, 3, 4, 5]\n",
    "  \n",
    "# calculating length of array\n",
    "N = len(arr)\n",
    "  \n",
    "ans =_findSum(arr,N)\n",
    "print (ans)"
   ]
